{
  "name": "@acme/api-client",
  "version": "0.1.0",
  "private": true,
  "description": "API client and type definitions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "generate": "openapi-typescript schema.yaml -o src/types/api.ts"
  },
  "dependencies": {
    "ky": "^1.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "openapi-typescript": "^6.7.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "eslint": "^8.55.0"
  }
}